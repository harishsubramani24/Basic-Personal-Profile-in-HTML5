echo 'pipeline {
    agent any

    stages {
        stage("Build") {
            steps {
                echo "Building HTML project"
            }
        }
        stage("Test") {
            steps {
                echo "Running tests"
            }
        }
        stage("Deploy") {
            steps {
                echo "Deploying HTML project"
            }
        }
    }
}' > Jenkinsfile
